<ess:page name="${qsheet.name}" title="${qsheet.title}" xmlns:ess="https://bitbucket.org/mhall/experiment-support-system" xmlns:py="http://genshi.edgewall.org/">
  <ess:styles>${qsheet.styles}</ess:styles>
  <ess:scripts>${qsheet.scripts}</ess:scripts>
  <ess:attribute py:for="attr in qsheet.attributes" py:if="attr.value" name="${attr.key}">${attr.value}</ess:attribute>
